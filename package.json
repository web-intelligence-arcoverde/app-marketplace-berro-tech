{
  "name": "berrotech",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-pixel": "emulator -avd Pixel_6_Pro_API_30",
    "custom-ios":"npx pod-install && npx react-native run-ios --simulator=iPhone 13",
    "ios": "npx pod-install && react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android:dev:release": "cd ./android && ./gradlew assembleRelease",
    "build:android:dev:aab": "cd ./android && ./gradlew bundleRelease",
    "build:ios:prod:release": "cd ./ios && /usr/bin/xcodebuild -sdk iphoneos -configuration Prod -workspace BerroTech.xcworkspace -scheme BerroTech_prod build && /usr/bin/xcodebuild -exportArchive -archivePath output/iphoneos/Prod/archive/BerroTech.xcarchive -exportPath output/iphoneos/Prod/export -exportOptionsPlist ios/ExportOptions.plist",
    "build:ios:hml:release": "cd ./ios && /usr/bin/xcodebuild -sdk iphoneos -configuration Hml -workspace BerroTech.xcworkspace -scheme BerroTech_hml build && /usr/bin/xcodebuild -exportArchive -archivePath output/iphoneos/Hml/archive/BerroTech.xcarchive -exportPath output/iphoneos/Hml/export -exportOptionsPlist ios/ExportOptionsHom.plist",
    "clear:ios": "cd ios && xcodebuild clean && cd ../",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.5",
    "@hookform/resolvers": "^2.9.11",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-picker/picker": "^2.4.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@redux-saga/core": "^1.2.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@testing-library/react-native": "^11.5.1",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/styled-components": "^4.1.1",
    "axios": "^1.2.5",
    "date-fns": "^2.29.3",
    "deprecated-react-native-prop-types": "^4.1.0",
    "latinize": "^0.5.0",
    "react": "18.2.0",
    "react-hook-form": "^7.43.4",
    "react-native": "0.71.8",
    "react-native-confirmation-code-field": "^7.3.1",
    "react-native-date-picker": "^4.2.13",
    "react-native-device-info": "^10.3.0",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-fbsdk-next": "^11.2.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-picker": "^5.1.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-lightweight-responsive": "^0.0.6",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mask-text": "^0.13.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-normalize": "^1.0.1",
    "react-native-reanimated": "^3.0.1",
    "react-native-responsive-pixels": "^0.5.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-select-dropdown": "^3.3.2",
    "react-native-snap-carousel": "^4.0.0-beta.6",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^13.8.0",
    "react-native-swiper": "^1.6.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-redux": "^8.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.2",
    "styled-components": "^4.1.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@rnx-kit/align-deps": "^2.2.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/latinize": "^0.2.15",
    "@types/react": "^18.0.24",
    "@types/react-native-video": "^5.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "navigation/native",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens",
        "svg"
      ]
    }
  }
}
