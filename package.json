    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.2",
    "styled-components": "^4.1.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
